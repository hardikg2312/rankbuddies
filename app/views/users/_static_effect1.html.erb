<center><%= image_tag("user_new/collapse.png", :alt => "...", :class => "user_new_effect_expand_img user_new_effect_collapse-1")%></center>

<script type="text/javascript">
   $(".user_new_effect_collapse-1").click(function(){
      var div_height = $(".user_new_effect-1").height();
      var height = $(window).height()-div_height;
      $(".user_new_effect-1").animate({height:height});
      $(".user_new_effect-1").empty().html("<center><img src='assets/user_new/expand.png' alt='...' class='user_new_effect_expand_img user_new_effect_expand-1'></center>");
  });
</script>