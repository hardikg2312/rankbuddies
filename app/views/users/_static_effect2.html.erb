<center><%= image_tag("user_new/collapse.png", :alt => "...", :class => "user_new_effect_expand_img user_new_effect_collapse-2")%></center>

<script type="text/javascript">
   $(".user_new_effect_collapse-2").click(function(){
      var div_height = $(".user_new_effect-2").height();
      var height = $(window).height()-div_height-40; 
      $(".user_new_effect-2").animate({height:height});
      $(".user_new_effect-2").empty().html("<center><img src='assets/user_new/expand.png' alt='...' class='user_new_effect_expand_img user_new_effect_expand-2'></center>");
  });
</script>